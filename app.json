{
  "expo": {
    "name": "Crodinger",
    "slug": "SafeZone",
    "version": "1.0.0",
    "scheme": "SafeZone",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "@react-native-firebase/app", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_full_big.png",
      "resizeMode": "contain",
      "backgroundColor": "#113842"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_full_big.png",
        "backgroundColor": "#113842"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.anonymous.SafeZone"
    }
  }
}
